{
    "dataset_choice": "4x",
    "learning_rate": 0.0001,
    "max_epochs": 8000,
    "chosen_scheduler": "none",
    "scheduler": {
        "scheduler_RLOP": {
            "type": "RLOP",
            "params": {
                "mode": "max",
                "patience": 10,
                "factor": 0.1,
                "verbose": true,
                "threshold": 1e-4 ,
                "min_lr": 1e-6
            }
        },
        "scheduler_cosinA": {
            "type": "cosinA",
            "params": {
                "T_max": 25,
                "eta_min": 1e-6,
                "verbose": true
            }
        },
        "scheduler_cosinAWR": {
            "type": "cosinAWR",
            "params": {
                "T_0": 25,
                "T_mult": 1,
                "eta_min": 1e-6,
                "verbose": true
            }
        }
    }
    
}



